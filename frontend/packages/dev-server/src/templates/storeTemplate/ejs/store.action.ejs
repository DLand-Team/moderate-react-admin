import { AddPageParams } from "@/services/apis/devApi";
import { devApi } from "@/services/apis/index";

const actions = {
  createList: async () => {
    const { data } = await devApi.fetchPageList();
    return {
      pageList: data.pageList,
    };
  },
  add: async (params: AddPageParams) => {
    await devApi.addPageList(params);
  },
};

export default actions;
