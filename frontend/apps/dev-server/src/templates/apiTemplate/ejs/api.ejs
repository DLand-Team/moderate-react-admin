<%
  const name = locals.name
-%>

import { http } from "@/common/http";
import { PageType } from "../stores/devStore/devStore.model";

export interface AddPageParams {
  pageName: string;
  pagePath: string;
  pageRemark: string;
}

export interface FetchPageListRes {
  pageList: PageType[];
  exportList: string[];
}


// 查询文章列表，post
function fetchPageList() {
  return http.request<FetchPageListRes>({
    url: "/api/dev/getPageList",
    method: "GET",
  });
}

// 添加页面
function addPageList(params: AddPageParams) {
  return http.request({
    url: "/api/dev/addPage",
    method: "POST",
    data: params,
  });
}


const <%- name %> = {
  fetchPageList,
  addPageList,
};

export default <%- name %>;
